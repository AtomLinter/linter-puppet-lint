{
  "name": "linter-puppet-lint",
  "main": "./lib/main",
  "version": "0.7.0",
  "description": "Lint puppet on save using puppet-lint",
  "repository": "https://github.com/AtomLinter/linter-puppet-lint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-puppet-lint/issues"
  },
  "engines": {
    "atom": ">=1.4.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "atom-linter": "^6.0.0"
  },
  "package-deps": [
    "linter",
    "language-puppet"
  ],
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "globals": {
      "atom": true
    }
  }
}
