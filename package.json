{
  "name": "linter-puppet-lint",
  "main": "./lib/main.js",
  "version": "0.7.0",
  "description": "Lint puppet on save using puppet-lint",
  "repository": "https://github.com/AtomLinter/linter-puppet-lint",
  "license": "MIT",
  "private": true,
  "keywords": [
    "lint",
    "linter",
    "puppet-lint"
  ],
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-puppet-lint/issues"
  },
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "atom-linter": "^8.0.0"
  },
  "package-deps": [
    "linter",
    "language-puppet"
  ],
  "devDependencies": {
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.1.0",
    "eslint-plugin-import": "^1.15.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "atom": true
    }
  }
}
