{
  "name": "linter-puppet-lint",
  "main": "./lib/main",
  "version": "0.6.2",
  "description": "Lint puppet on save using puppet-lint",
  "repository": "https://github.com/AtomLinter/linter-puppet-lint",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-package-deps": "^3.0.7",
    "atom-linter": "^4.3.0"
  },
  "package-deps": [
    "linter"
  ],
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
